<div class="container">
  <%= render "page_header", title: 'Login', button: 'no' %>
  <%= render "devise_form", 
  model: @user, 
  url: session_path(@user),
  action: 'Login',
  fields_display: 'col',
  fields: [
    {
      name: 'email',
      type: :email_field,
      class_name: 'col-12 col-sm-4',
    },
    {
      name: 'password',
      type: :password_field,
      class_name: 'col-12 col-sm-4',
    },
  ] %>
</div>



